{
    "typescript.tsc.autoDetect": "off",
    "files.autoSave": "off",
    "editor.formatOnSave": true,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "prettier.singleQuote": true,
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "files.exclude": {
        "**/.gitignore": false,
        "**/.git": true,
        "**/.terraform": true,
        "**/.tfstate": true,
        "**/.npmrc": true,
        "**/dev/dvc-command": true,
        "**/.env.backup": true,
        "**/.dockerignore": true,
        "**/dvc-command": true,
        "**/gha**": true,
        "**/package-lock.json": true,
        "**/node_modules": true,
        "**/__pycache__": true,
        "**/.next": true,
        "**/dist": false,
        "**/.terraform.lock.hcl": true,
        "**/backend.tf": true,
        "**/versions.tf": true,
        "**/.npmignore": true,
        "**/.deno_cache": true
    },
    "files.associations": {
        "*.json": "jsonc"
    },
    "deno.enable": true,
    "deno.lint": true,
    "deno.cacheOnSave": true,
    "deno.path": "/deno/bin/deno",
    "deno.cache": "/deno/.cache",
    "deno.config": "/home/vscode/deno.json",
    "deno.disablePaths": [
        "/workspaces/hide-unhide/app"
    ],
